      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none">
      <font size="20" style="1"/>
    </border>
    <children>
      <grid id="8da6d" binding="replicasPanel" layout-manager="GridBagLayout">
